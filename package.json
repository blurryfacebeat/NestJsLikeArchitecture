{
  "name": "nestjslikearchitecture",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx nodemon",
    "build": "rimraf ./build && npx tsc",
    "start": "npm run build && node build/index",
    "lint": "npx eslint ./src",
    "format": "npx eslint ./src --fix"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.31.0",
    "husky": "^8.0.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "tslog": "^4.7.1"
  }
}
